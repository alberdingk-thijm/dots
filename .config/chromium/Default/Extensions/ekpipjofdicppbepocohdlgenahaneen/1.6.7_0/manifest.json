{
   "background": {
      "scripts": [ "js/background.js" ]
   },
   "browser_action": {
      "default_icon": "icon38.png",
      "default_popup": "html/popup.html",
      "default_title": "Streamkeys"
   },
   "commands": {
      "dislike": {
         "description": "Toggle dislike",
         "global": true
      },
      "like": {
         "description": "Toggle like",
         "global": true
      },
      "mute": {
         "description": "Toggle mute",
         "global": true,
         "suggested_key": {
            "default": "Ctrl+Shift+2",
            "mac": "MacCtrl+Shift+2"
         }
      },
      "playNext": {
         "description": "Play next track",
         "global": true,
         "suggested_key": {
            "default": "MediaNextTrack"
         }
      },
      "playPause": {
         "description": "Toggle play/pause",
         "global": true,
         "suggested_key": {
            "default": "MediaPlayPause"
         }
      },
      "playPrev": {
         "description": "Play previous track",
         "global": true,
         "suggested_key": {
            "default": "MediaPrevTrack"
         }
      },
      "stop": {
         "description": "Stop all players",
         "global": true,
         "suggested_key": {
            "default": "MediaStop"
         }
      }
   },
   "content_scripts": [ {
      "js": [ "js/contentscript.js" ],
      "matches": [ "http://*/*", "https://*/*" ]
   }, {
      "js": [ "js/inject/sksite_inject.js" ],
      "matches": [ "*://*.streamkeys.com/*" ]
   } ],
   "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
   "description": "Global hotkeys for online music players including support for media keys.",
   "homepage_url": "http://www.streamkeys.com",
   "icons": {
      "128": "icon128.png",
      "48": "icon48.png"
   },
   "key": "MIGfMA0GCSqGSIb3DQEBAQUAA4GNADCBiQKBgQCNbjYBFhICyQewzxN7JIg6ziE7UQ14gnULafHwUYpCUouWxPqVa6hOGDVjOwF42COA9Cxm58QBo2k1taN1mKbVEsdQ1yBaRnnBKCFikbHbVqss9soT439HZw1y4YxyrH56hJ+5arlIeQKlntSz9Cliw+q/9KFnXkf9pHB0F5wREwIDAQAB",
   "manifest_version": 2,
   "name": "Streamkeys",
   "options_page": "html/options.html",
   "permissions": [ "tabs", "storage", "http://*/*", "https://*/*" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.6.7",
   "web_accessible_resources": [ "js/inject/devchat_inject.js", "js/inject/seesu_inject.js", "js/inject/palcomp3_inject.js", "js/inject/laracasts_inject.js", "js/inject/hoopla_inject.js", "js/controllers/*" ]
}
